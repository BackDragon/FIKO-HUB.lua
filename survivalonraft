TabTP:CreateSection("‡∏ß‡∏≤‡∏£‡πå‡∏õ")

local player = LP
local camera = workspace.CurrentCamera
local targetPlayer
local spectating = false

local function GetPlayerList()
	local names = {}
	for _, plr in pairs(Players:GetPlayers()) do
		if plr ~= player then
			table.insert(names, plr.Name)
		end
	end
	return names
end

local PlayerDropd= TabTP:CreateDropdown({
	Name = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
	Options = GetPlayerList(),
	CurrentOption = {},
	MultipleOptions = false,
	Flag = "PlayerTeleportDropdown",
	Callback = function(Option)
		targetPlayer = Players:FindFirstChild(Option[1])
	end
})

TabTP:CreateButton({
	Name = "‚ö° ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
	Callback = function()
		if not targetPlayer then
			return Rayfield:Notify({
				Title = "‚ö†Ô∏è ‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
				Content = "‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ä‡∏∑‡πà‡∏≠‡∏à‡∏≤‡∏Å‡∏£‡∏≤‡∏¢‡∏Å‡∏≤‡∏£‡∏Å‡πà‡∏≠‡∏ô",
				Duration = 2
			})
		end
		local myChar = player.Character or player.CharacterAdded:Wait()
		local myRoot = myChar:FindFirstChild("HumanoidRootPart")
		local targetChar = targetPlayer.Character or targetPlayer.CharacterAdded:Wait()
		local targetRoot = targetChar:FindFirstChild("HumanoidRootPart")

		if myRoot and targetRoot then
			myRoot.CFrame = targetRoot.CFrame + Vector3.new(0, 3, 0)
			Rayfield:Notify({
				Title = "‚úÖ ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!",
				Content = "‡∏Ñ‡∏∏‡∏ì‡∏ñ‡∏π‡∏Å‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏´‡∏≤ " .. targetPlayer.Name,
				Duration = 2
			})
		else
			Rayfield:Notify({
				Title = "‚ùå ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏•‡πâ‡∏°‡πÄ‡∏´‡∏•‡∏ß",
				Content = "‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏Ç‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢",
				Duration = 3
			})
		end
	end
})

TabTP:CreateButton({
	Name = "üëÅÔ∏è ‡πÄ‡∏õ‡∏¥‡∏î / ‡∏õ‡∏¥‡∏î‡πÇ‡∏´‡∏°‡∏î‡∏°‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
	Callback = function()
		if not targetPlayer then
			return Rayfield:Notify({
				Title = "‚ö†Ô∏è ‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
				Content = "‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ä‡∏∑‡πà‡∏≠‡∏à‡∏≤‡∏Å Dropdown ‡∏Å‡πà‡∏≠‡∏ô",
				Duration = 2
			})
		end
		spectating = not spectating
		if spectating then
			camera.CameraSubject = targetPlayer.Character:WaitForChild("Humanoid")
			camera.CameraType = Enum.CameraType.Custom
			Rayfield:Notify({
				Title = "üé• ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏°‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
				Content = "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏°‡∏≠‡∏á: " .. targetPlayer.Name,
				Duration = 2
			})
			targetPlayer.CharacterAdded:Connect(function(newChar)
				if spectating then
					task.wait(1)
					camera.CameraSubject = newChar:WaitForChild("Humanoid")
				end
			end)
		else
			camera.CameraSubject = player.Character:WaitForChild("Humanoid")
			camera.CameraType = Enum.CameraType.Custom
			Rayfield:Notify({
				Title = "üëÅÔ∏è ‡∏´‡∏¢‡∏∏‡∏î‡∏°‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
				Content = "‡∏Å‡∏•‡∏±‡∏ö‡∏°‡∏≤‡∏°‡∏≠‡∏á‡∏ï‡∏±‡∏ß‡πÄ‡∏≠‡∏á‡πÅ‡∏•‡πâ‡∏ß",
				Duration = 2
			})
		end
	end
})

Players.PlayerAdded:Connect(function()
	task.wait(1)
	PlayerDropdown:Refresh(GetPlayerList(), true)
end)

Players.PlayerRemoving:Connect(function()
	task.wait(1)
	PlayerDropdown:Refresh(GetPlayerList(), true)
end)

-- üß≠ ‡∏ï‡πâ‡∏≠‡∏á‡∏°‡∏µ‡πÅ‡∏ó‡πá‡∏ö‡∏Å‡πà‡∏≠‡∏ô
-- local TabMunu = Window:CreateTab("‡πÅ‡∏°‡∏û‡∏•‡∏≠‡∏¢‡πÅ‡∏û")

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local autoCollect = false

-------------------------------------------------
-- üì¶ ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏Å‡πá‡∏ö‡∏Å‡∏•‡πà‡∏≠‡∏á
-------------------------------------------------
local function collectBoxes()
    local world = workspace:FindFirstChild("World")
    if not world or not world:FindFirstChild("Items") then return end

    local boxes = world.Items:GetChildren()
    local foundBox = false

    for _, box in pairs(boxes) do
        local prompt = box:FindFirstChildOfClass("ProximityPrompt")
        if prompt then
            foundBox = true
            character:MoveTo(box.Position)
            task.wait(0.3)
            pcall(function()
                fireproximityprompt(prompt)
            end)
            task.wait(0.3)
        end
    end

    -- ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏Å‡∏•‡πà‡∏≠‡∏á ‚Üí ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏Å‡∏•‡∏±‡∏ö Spawn
    if not foundBox and workspace:FindFirstChild("SpawnLocation") then
        local spawnPos = workspace.SpawnLocation.Position
        character:MoveTo(spawnPos)
    end
end

-------------------------------------------------
-- üîÅ Auto ‡πÄ‡∏Å‡πá‡∏ö‡∏Å‡∏•‡πà‡∏≠‡∏á
-------------------------------------------------
TabMenu:CreateToggle({
    Name = "üîÅ Auto ‡πÄ‡∏Å‡πá‡∏ö‡∏Å‡∏•‡πà‡∏≠‡∏á + ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏Å‡∏•‡∏±‡∏ö Spawn",
    CurrentValue = false,
    Callback = function(Value)
        autoCollect = Value

        if autoCollect then
            Rayfield:Notify({
                Title = "Auto Collect",
                Content = "‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡πÅ‡∏•‡πâ‡∏ß!",
                Duration = 2
            })
            task.spawn(function()
                while autoCollect do
                    collectBoxes()
                    task.wait(2)
                end
            end)
        else
            Rayfield:Notify({
                Title = "Auto Collect",
                Content = "‡∏´‡∏¢‡∏∏‡∏î‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡πÅ‡∏•‡πâ‡∏ß!",
                Duration = 2
            })
        end
    end
})

-------------------------------------------------
-- üè† ‡∏õ‡∏∏‡πà‡∏°‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏Å‡∏•‡∏±‡∏ö Spawn (‡πÅ‡∏¢‡∏Å‡∏ï‡πà‡∏≤‡∏á‡∏´‡∏≤‡∏Å)
-------------------------------------------------
TabMenu:CreateButton({
    Name = "üè† ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏Å‡∏•‡∏±‡∏ö‡∏à‡∏∏‡∏î‡πÄ‡∏Å‡∏¥‡∏î",
    Callback = function()
        local character = player.Character or player.CharacterAdded:Wait()
        local root = character:FindFirstChild("HumanoidRootPart")

        if workspace:FindFirstChild("SpawnLocation") and root then
            root.CFrame = workspace.SpawnLocation.CFrame + Vector3.new(0, 3, 0)
            Rayfield:Notify({
                Title = "‚úÖ ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!",
                Content = "‡∏Ñ‡∏∏‡∏ì‡∏Å‡∏•‡∏±‡∏ö‡∏°‡∏≤‡∏¢‡∏±‡∏á‡∏à‡∏∏‡∏î‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡πÅ‡∏•‡πâ‡∏ß",
                Duration = 3
            })
        else
            Rayfield:Notify({
                Title = "‚ö†Ô∏è ‡πÑ‡∏°‡πà‡∏û‡∏ö SpawnLocation",
                Content = "‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏à‡∏∏‡∏î‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏ô‡πÅ‡∏°‡∏õ‡∏ô‡∏µ‡πâ",
                Duration = 3
            })
        end
    end
})

-------------------------------------------------
-- üì¶ ‡πÇ‡∏´‡∏•‡∏î‡∏Å‡∏≤‡∏£‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤
-------------------------------------------------
