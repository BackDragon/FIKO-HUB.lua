local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏´‡∏ô‡πâ‡∏≤‡∏ï‡πà‡∏≤‡∏á
local Window = Rayfield:CreateWindow({
Name = "FIKO HUB",
Icon = 0,
LoadingTitle = "‡πÄ‡∏°‡∏ô‡∏π‡∏´‡∏•‡∏±‡∏Å",
LoadingSubtitle = "FIKO HUB",
Theme = "Default",

DisableRayfieldPrompts = true,
DisableBuildWarnings = true,

ConfigurationSaving = {
Enabled = true,
FolderName = "FIKO HUB",
FileName = "FIKO HUB"
},

Discord = {
Enabled = false,
Invite = "https://discord.gg/U8qEGaNe6E",
RememberJoins = true
},

KeySystem = false
})

local TabPlayer= Window:CreateTab("‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô")
local TabESP   = Window:CreateTab("ESP")
local TabDiscord=Window:CreateTab("Discord")

TabDiscord:CreateSection("‡∏Å‡∏î‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏≠‡∏≤‡∏•‡∏¥‡πâ‡∏á‡∏î‡∏¥‡∏™")
TabDiscord:CreateButton({
    Name = "üìã Copy Discord Link",
    Callback = function()
        local link = "https://discord.gg/U8qEGaNe6E"
        if setclipboard then
            setclipboard(link)
            game.StarterGui:SetCore("SendNotification", {
                Title = "‚úÖ ‡∏•‡∏¥‡∏á‡∏Å‡πå‡∏ñ‡∏π‡∏Å‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡πÅ‡∏•‡πâ‡∏ß!",
                Text = link,
                Duration = 5
            })
        else
            game.StarterGui:SetCore("SendNotification", {
                Title = "‚ö†Ô∏è ‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡πÑ‡∏î‡πâ",
                Text = "Executor ‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì‡πÑ‡∏°‡πà‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö setclipboard",
                Duration = 5
            })
        end
    end,
})

-------------------------------------------------
-- ‚öôÔ∏è ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß + ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏™‡∏π‡∏á
-------------------------------------------------
TabPlayer:CreateSection("‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏ï‡πà‡∏≤‡∏á‡πÜ")

-- üèÉ‚Äç‚ôÇÔ∏è ‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß
local SpeedLockToggle = TabPlayer:CreateToggle({
	Name = "‡πÄ‡∏õ‡∏¥‡∏î‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß",
	CurrentValue = false,
	Flag = "SpeedLock",
	Callback = function(state)
		speedEnforced = state
		local h = speedHumanoid
		if not h or not h.Parent then return end
		if state then
			if not speedTickConn then
				speedLastTick = 0
				speedTickConn = RunService.Heartbeat:Connect(heartbeat)
			end
			if canEnforce() then setWalkSpeed(h, speedCurrent) end
			Rayfield:Notify({
				Title = "Speed",
				Content = "‚úÖ ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß‡πÅ‡∏•‡πâ‡∏ß",
				Duration = 3
			})
		else
			disconnectAll()
			applyDisabledState()
			Rayfield:Notify({
				Title = "Speed",
				Content = "‚ùå ‡∏õ‡∏¥‡∏î‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß‡πÅ‡∏•‡πâ‡∏ß",
				Duration = 3
			})
		end
	end
})

-- üîß ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡πà‡∏≤‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß
TabPlayer:CreateSlider({
	Name = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß",
	Range = {0, 200},
	Increment = 1,
	CurrentValue = 16,
	Flag = "WalkSpeed",
	Callback = function(v)
		speedCurrent = v
		if speedEnforced and canEnforce() then
			setWalkSpeed(speedHumanoid, speedCurrent)
		end
	end
})

-- ü¶ò ‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡πà‡∏≤‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏™‡∏π‡∏á
local jumpPowerCurrent = 50
TabPlayer:CreateSlider({
	Name = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î",
	Range = {0, 300},
	Increment = 5,
	CurrentValue = 50,
	Flag = "JumpPower",
	Callback = function(v)
		jumpPowerCurrent = v
		local h = speedHumanoid
		if h and h.Parent then
			h.JumpPower = v
		end
	end
})

-- ‚ôªÔ∏è ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ó‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß + ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏™‡∏π‡∏á
TabPlayer:CreateButton({
	Name = "‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ó‡∏Ñ‡πà‡∏≤‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÅ‡∏•‡∏∞‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î",
	Callback = function()
		speedCurrent = canonicalDefault()
		jumpPowerCurrent = 50
		local h = speedHumanoid
		if h and h.Parent then
			h.WalkSpeed = speedCurrent
			h.JumpPower = jumpPowerCurrent
			Rayfield:Notify({
				Title = "‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ó‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à",
				Content = "‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÅ‡∏•‡∏∞‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏Å‡∏•‡∏±‡∏ö‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡πÅ‡∏•‡πâ‡∏ß ‚úÖ",
				Duration = 3
			})
		end
	end
})

Rayfield:LoadConfiguration()
