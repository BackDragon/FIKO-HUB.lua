-- üß† ‡∏™‡πà‡∏ß‡∏ô‡∏´‡∏±‡∏ß‡∏´‡∏•‡∏±‡∏Å (FIKO HUB)
-------------------------------------------------
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local Lighting = game:GetService("Lighting")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local StarterGui = game:GetService("StarterGui")
local VirtualUser = game:GetService("VirtualUser")
local LP = Players.LocalPlayer

-------------------------------------------------
-- ‚öôÔ∏è ‡πÇ‡∏´‡∏•‡∏î Rayfield UI
-------------------------------------------------
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-------------------------------------------------
-- ü™ü ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏´‡∏ô‡πâ‡∏≤‡∏ï‡πà‡∏≤‡∏á‡∏´‡∏•‡∏±‡∏Å
-------------------------------------------------
local Window = Rayfield:CreateWindow({
	Name = "FIKO HUB",
	Icon = 0,
	LoadingTitle = "‡πÄ‡∏°‡∏ô‡∏π‡∏´‡∏•‡∏±‡∏Å",
	LoadingSubtitle = "FIKO HUB",
	Theme = "Default",

	DisableRayfieldPrompts = true,
	DisableBuildWarnings = true,

	ConfigurationSaving = {
		Enabled = true,
		FolderName = "FIKO HUB",
		FileName = "FIKO HUB"
	},

	Discord = {
		Enabled = false,
		Invite = "https://discord.gg/U8qEGaNe6E",
		RememberJoins = true
	},

	KeySystem = false
})

-------------------------------------------------
-- üìÅ ‡πÅ‡∏ó‡πá‡∏ö‡∏ï‡πà‡∏≤‡∏á ‡πÜ
-------------------------------------------------
local TabPlayer = Window:CreateTab("‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô")
local TabESP = Window:CreateTab("ESP")
local TabTP = Window:CreateTab("TP")


-------------------------------------------------
-- ‚öôÔ∏è ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß + ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î
-------------------------------------------------
TabPlayer:CreateSection("‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏ï‡πà‡∏≤‡∏á‡πÜ")

local SpeedSlider = TabPlayer:CreateSlider({
	Name = "‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß",
	Range = {16, 700},
	Increment = 1,
	Suffix = "Speed",
	CurrentValue = 16,
	Callback = function(Value)
		local humanoid = LP.Character and LP.Character:FindFirstChildOfClass("Humanoid")
		if humanoid then
			humanoid.WalkSpeed = Value
		end
	end
})

local JumpSlider = TabPlayer:CreateSlider({
	Name = "‡∏õ‡∏£‡∏±‡∏ö‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏π‡∏á‡∏Å‡∏≤‡∏£‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î",
	Range = {50, 500},
	Increment = 10,
	Suffix = "Power",
	CurrentValue = 50,
	Callback = function(Value)
		local humanoid = LP.Character and LP.Character:FindFirstChildOfClass("Humanoid")
		if humanoid then
			humanoid.UseJumpPower = true
			humanoid.JumpPower = Value
		end
	end
})

TabPlayer:CreateButton({
	Name = "‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏Ñ‡πà‡∏≤‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î",
	Callback = function()
		local humanoid = LP.Character and LP.Character:FindFirstChildOfClass("Humanoid")
		if humanoid then
			humanoid.WalkSpeed = 16
			humanoid.JumpPower = 50
		end
		SpeedSlider:Set(16)
		JumpSlider:Set(50)

		Rayfield:Notify({
			Title = "‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!",
			Content = "‡∏Å‡∏•‡∏±‡∏ö‡πÑ‡∏õ‡∏Ñ‡πà‡∏≤‡∏õ‡∏Å‡∏ï‡∏¥‡πÅ‡∏•‡πâ‡∏ß ‚úÖ",
			Duration = 3,
			Image = 4483362458
		})
	end
})

-------------------------------------------------
-- üß± ‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏∞‡∏•‡∏∏ (Noclip)
-------------------------------------------------
TabPlayer:CreateSection("‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏∞‡∏•‡∏∏")

local noclipEnabled = false

TabPlayer:CreateButton({
	Name = "‡πÄ‡∏õ‡∏¥‡∏î / ‡∏õ‡∏¥‡∏î ‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏∞‡∏•‡∏∏",
	Callback = function()
		noclipEnabled = not noclipEnabled
		local status = noclipEnabled and "‚úÖ ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô" or "‚ùå ‡∏õ‡∏¥‡∏î‡πÅ‡∏•‡πâ‡∏ß"
		Rayfield:Notify({
			Title = "Noclip",
			Content = status,
			Duration = 3
		})
	end
})

RunService.Stepped:Connect(function()
	if noclipEnabled and LP.Character then
		for _, part in pairs(LP.Character:GetChildren()) do
			if part:IsA("BasePart") then
				part.CanCollide = false
			end
		end
	end
end)

-------------------------------------------------
-- üí° FullBright
-------------------------------------------------
TabPlayer:CreateSection("‡πÅ‡∏™‡∏á‡∏™‡∏ß‡πà‡∏≤‡∏á")

local FullBrightEnabled = false
local FBConnection

TabPlayer:CreateButton({
	Name = "üîÜ ‡πÄ‡∏õ‡∏¥‡∏î‡πÇ‡∏´‡∏°‡∏î‡πÅ‡∏°‡∏û‡∏™‡∏ß‡πà‡∏≤‡∏á (FullBright)",
	Callback = function()
		FullBrightEnabled = not FullBrightEnabled

		if FullBrightEnabled then
			FBConnection = RunService.RenderStepped:Connect(function()
				Lighting.Brightness = 2
				Lighting.ClockTime = 14
				Lighting.FogEnd = 1e10
				Lighting.GlobalShadows = false
				Lighting.OutdoorAmbient = Color3.new(1, 1, 1)
			end)
			Rayfield:Notify({
				Title = "‡πÄ‡∏õ‡∏¥‡∏î FullBright ‚òÄÔ∏è",
				Content = "‡πÅ‡∏°‡∏õ‡∏™‡∏ß‡πà‡∏≤‡∏á‡∏ñ‡∏≤‡∏ß‡∏£‡πÅ‡∏•‡πâ‡∏ß",
				Duration = 2
			})
		else
			if FBConnection then FBConnection:Disconnect() end
			Lighting.GlobalShadows = true
			Rayfield:Notify({
				Title = "‡∏õ‡∏¥‡∏î FullBright üåë",
				Content = "‡∏Å‡∏•‡∏±‡∏ö‡∏™‡∏π‡πà‡∏™‡∏†‡∏≤‡∏û‡∏õ‡∏Å‡∏ï‡∏¥",
				Duration = 2
			})
		end
	end
})

-------------------------------------------------
-- üïäÔ∏è ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ö‡∏¥‡∏ô
-------------------------------------------------
TabPlayer:CreateSection("‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ö‡∏¥‡∏ô")
TabPlayer:CreateButton({
	Name = "‡∏Å‡∏î‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏¥‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏ö‡∏¥‡∏ô",
	Callback = function()
		local url = "https://gist.githubusercontent.com/meozoneYT/bf037dff9f0a70017304ddd67fdcd370/raw/e14e74f425b060df523343cf30b787074eb3c5d2/Arceus%20X%20Fly%20OBF.lua"
		loadstring(game:HttpGet(url))()
	end
})

-------------------------------------------------
-- üß≠ ‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï / ‡∏°‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
-------------------------------------------------
TabTP:CreateSection("‡∏ß‡∏≤‡∏£‡πå‡∏õ")

local player = LP
local camera = workspace.CurrentCamera
local targetPlayer
local spectating = false

local function GetPlayerList()
	local names = {}
	for _, plr in pairs(Players:GetPlayers()) do
		if plr ~= player then
			table.insert(names, plr.Name)
		end
	end
	return names
end

local PlayerDropdown = TabTP:CreateDropdown({
	Name = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
	Options = GetPlayerList(),
	CurrentOption = {},
	MultipleOptions = false,
	Flag = "PlayerTeleportDropdown",
	Callback = function(Option)
		targetPlayer = Players:FindFirstChild(Option[1])
	end
})

TabTP:CreateButton({
	Name = "‚ö° ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏´‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
	Callback = function()
		if not targetPlayer then
			return Rayfield:Notify({
				Title = "‚ö†Ô∏è ‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
				Content = "‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ä‡∏∑‡πà‡∏≠‡∏à‡∏≤‡∏Å‡∏£‡∏≤‡∏¢‡∏Å‡∏≤‡∏£‡∏Å‡πà‡∏≠‡∏ô",
				Duration = 2
			})
		end
		local myChar = player.Character or player.CharacterAdded:Wait()
		local myRoot = myChar:FindFirstChild("HumanoidRootPart")
		local targetChar = targetPlayer.Character or targetPlayer.CharacterAdded:Wait()
		local targetRoot = targetChar:FindFirstChild("HumanoidRootPart")

		if myRoot and targetRoot then
			myRoot.CFrame = targetRoot.CFrame + Vector3.new(0, 3, 0)
			Rayfield:Notify({
				Title = "‚úÖ ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!",
				Content = "‡∏Ñ‡∏∏‡∏ì‡∏ñ‡∏π‡∏Å‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏´‡∏≤ " .. targetPlayer.Name,
				Duration = 2
			})
		else
			Rayfield:Notify({
				Title = "‚ùå ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏•‡πâ‡∏°‡πÄ‡∏´‡∏•‡∏ß",
				Content = "‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏Ç‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢",
				Duration = 3
			})
		end
	end
})

TabTP:CreateButton({
	Name = "üëÅÔ∏è ‡πÄ‡∏õ‡∏¥‡∏î / ‡∏õ‡∏¥‡∏î‡πÇ‡∏´‡∏°‡∏î‡∏°‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
	Callback = function()
		if not targetPlayer then
			return Rayfield:Notify({
				Title = "‚ö†Ô∏è ‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
				Content = "‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ä‡∏∑‡πà‡∏≠‡∏à‡∏≤‡∏Å Dropdown ‡∏Å‡πà‡∏≠‡∏ô",
				Duration = 2
			})
		end
		spectating = not spectating
		if spectating then
			camera.CameraSubject = targetPlayer.Character:WaitForChild("Humanoid")
			camera.CameraType = Enum.CameraType.Custom
			Rayfield:Notify({
				Title = "üé• ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏°‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
				Content = "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏°‡∏≠‡∏á: " .. targetPlayer.Name,
				Duration = 2
			})
			targetPlayer.CharacterAdded:Connect(function(newChar)
				if spectating then
					task.wait(1)
					camera.CameraSubject = newChar:WaitForChild("Humanoid")
				end
			end)
		else
			camera.CameraSubject = player.Character:WaitForChild("Humanoid")
			camera.CameraType = Enum.CameraType.Custom
			Rayfield:Notify({
				Title = "üëÅÔ∏è ‡∏´‡∏¢‡∏∏‡∏î‡∏°‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
				Content = "‡∏Å‡∏•‡∏±‡∏ö‡∏°‡∏≤‡∏°‡∏≠‡∏á‡∏ï‡∏±‡∏ß‡πÄ‡∏≠‡∏á‡πÅ‡∏•‡πâ‡∏ß",
				Duration = 2
			})
		end
	end
})

Players.PlayerAdded:Connect(function()
	task.wait(1)
	PlayerDropdown:Refresh(GetPlayerList(), true)
end)

Players.PlayerRemoving:Connect(function()
	task.wait(1)
	PlayerDropdown:Refresh(GetPlayerList(), true)
end)

-------------------------------------------------
-- üì¶ ‡πÇ‡∏´‡∏•‡∏î‡∏Å‡∏≤‡∏£‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤
-------------------------------------------------
Rayfield:LoadConfiguration()
Rayfield:Notify({
	Title = "FIKO HUB",
	Content = "‡∏£‡∏∞‡∏ö‡∏ö‡πÄ‡∏°‡∏ô‡∏π‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à ‚úÖ",
	Duration = 4
})
